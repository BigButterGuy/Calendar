<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="design.voight.InterfaceController">
   <center>
      <AnchorPane prefHeight="1080.0" prefWidth="1920.0">
         <children>
            <ScrollPane hbarPolicy="ALWAYS" layoutY="-2.0" prefHeight="1096.0" prefWidth="1926.0" vbarPolicy="NEVER">
               <content>
                  <AnchorPane fx:id="ganttPane" prefHeight="1250.0" prefWidth="2771.0" />
               </content></ScrollPane>
         </children></AnchorPane>
   </center>
   <left>
      <VBox prefHeight="1080.0" prefWidth="335.0" spacing="10.0" BorderPane.alignment="TOP_LEFT">
         <children>
            <Label alignment="CENTER" prefHeight="45.0" prefWidth="494.0" text="Upcoming">
               <font>
                  <Font name="Calibri" size="39.0" />
               </font>
            </Label>
            <ListView prefHeight="293.0" prefWidth="335.0" />
            <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="38.0" prefWidth="458.0" text="Filter By">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <CheckBox mnemonicParsing="false" prefHeight="32.0" prefWidth="375.0" text="Date">
               <font>
                  <Font size="22.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </CheckBox>
            <CheckBox layoutX="10.0" layoutY="417.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="341.0" text="Color">
               <font>
                  <Font size="22.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </CheckBox>
            <CheckBox layoutX="10.0" layoutY="417.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="478.0" text="Severity">
               <font>
                  <Font size="22.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </CheckBox>
            <CheckBox layoutX="10.0" layoutY="459.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="489.0" text="Progress">
               <font>
                  <Font size="22.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </CheckBox>
            <Slider />
            <Button fx:id="onNewProjectButtonClick" contentDisplay="TOP" mnemonicParsing="false" onAction="#onNewProjectButtonClick" prefHeight="51.0" prefWidth="323.0" text="New Project">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Label fx:id="newProjectLabel" prefHeight="18.0" prefWidth="367.0" />
            <Button layoutX="10.0" layoutY="619.0" mnemonicParsing="false" prefHeight="51.0" prefWidth="323.0" text="Project List" />
         </children>
      </VBox>
   </left>
</BorderPane>
